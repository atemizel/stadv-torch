{
  "dockerFile": "docker/vscode.Dockerfile",
  "mounts": [
    "source=${localEnv:HOME}/.cache,target=/home/user/.cache,type=bind"
  ],
  "runArgs": ["--runtime", "nvidia", "--env-file", ".env"],
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance",
    "esbenp.prettier-vscode"
  ]
}
